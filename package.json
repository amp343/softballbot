{
  "name": "softballbot",
  "version": "0.1.0",
  "private": true,
  "author": "Andrew Peters <amp343@gmail.com>",
  "description": "A simple helpful robot for softball",
  "engines": {
    "node": "9.5.x"
  },
  "scripts": {
    "build": "yarn && tsc --emitDeclarationOnly && babel _src --out-dir src --extensions \".ts,.js\"",
    "start": "npm run build && ./bin/hubot",
    "start:dev": "docker-compose up -d && yarn start",
    "start:prod": "npm run build && ./bin/hubot -a slack"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "fast-csv": "^2.0.0",
    "google-spreadsheet": "^2.0.4",
    "googleapis": "^5.2.1",
    "hubot": "^2.10.0",
    "hubot-diagnostics": "0.0.1",
    "hubot-help": "^0.1.1",
    "hubot-heroku-keepalive": "0.0.4",
    "hubot-redis-brain": "0.0.2",
    "hubot-reload-scripts": "^0.1.4",
    "hubot-rules": "^0.1.0",
    "hubot-scripts": "^2.5.16",
    "hubot-slack": "^3.1.0",
    "jsdom": "^8.4.1",
    "lodash": "^4.17.10",
    "moment": "^2.13.0",
    "ramda": "^0.25.0",
    "request-promise": "^3.0.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-typescript": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@types/hubot": "^2.19.2",
    "@types/lodash": "^4.14.110",
    "@types/node": "^10.3.4",
    "@types/ramda": "^0.25.33",
    "@types/request-promise": "^4.1.41",
    "babel-plugin-import": "^1.8.0",
    "typescript": "^2.9.2"
  }
}
